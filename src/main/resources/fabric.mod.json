{
  "schemaVersion": 1,
  "id": "brew-addon",
  "version": "${version}",
  "name": "Brew Addon",
  "description": "A meteor client addon for the drunks.",
  "authors": [
    "Eglijohn",
    "R3akeOn3_"
  ],
  "contact": {
    "sources": "https://github.com/eglijohn/brew-addon"
  },
  "icon": "assets/brewaddon/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "blub.brewaddon.BrewAddon"
    ]
  },
  "mixins": [
    "brewaddon.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "252,186,3"
  },
  "depends": {
    "java": ">=21",
    "minecraft": ["${mc_version}"],
    "meteor-client": "*"
  }
}
